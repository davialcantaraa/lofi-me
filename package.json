{
	"name": "lofi-me",
	"version": "0.1.0",
	"main": "packages/electron/main.js",
	"private": "true",
	"workspaces": {
		"packages": [
			"packages/*"
		]
	},
	"scripts": {
		"app-start": "lerna run start --stream",
		"app-build": "lerna run build --stream",
		"app-test": "lerna run test --stream",
		"app-eject": "lerna run eject --stream",
		"electron-dev": "lerna run electron --stream",
		"electron-build": "lerna run electron-build --stream",
		"web-dev": "lerna run web --stream",
		"web-build": "lerna run web-build --stream",
		"server-dev": "lerna run dev --stream",
		"diff": "lerna dif --streamf"
	},
	"dependencies": {
		"lerna": "^4.0.0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
